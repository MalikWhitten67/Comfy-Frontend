{
    "rewrites": [
        {
            "source": "/auth/login",
            "destination": "/auth/login/index.html"
        },
        {
            "source": "/auth/register",
            "destination": "/auth/register/index.html"
        },
        {
            "source": "/cart",
            "destination": "/cart/index.html"
        },
        {
            "source": "/favorites",
            "destination": "/favorites/index.html"
        },
        {
            "source": "/member/orders",
            "destination": "/member/orders/index.html"
        },
        {
            "source": "/member/settings",
            "destination": "/member/settings/index.html"
        },
        {
            "source": "/payments/cancel/:checkoutID",
            "destination": "/payments/cancel/index.html"
        },
        {
            "source": "/auth/forgot-password/:token",
            "destination": "/auth/forgot-password/index.html"
        },
        {
            "source": "/payments/success/:checkoutID",
            "destination": "/payments/success/index.html"
        },
        {
            "source": "/products/:id",
            "destination": "/products/index.html"
        }
    ]
}
